type Diary = 
 record {
   content: text;
   createdAt: text;
   diaryLabel: text;
   id: nat;
 };
service : {
  createDiary: (nat, text, text, text) -> () oneway;
  getOwnedNFTs: (principal) -> (vec principal) query;
  mint: (text, text) -> (principal);
  readDiaries: () -> (vec Diary) query;
  removeDiaries: (nat) -> () oneway;
}
